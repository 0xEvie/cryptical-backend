{
  "name": "cryptical-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register  test/*",
    "coverage": "cross-env NODE_ENV=test PORT=4000 nyc mocha test/*",
    "publish-coverage": "nyc report --reporter=text-lcov | coveralls",
    "build": "babel ./ --out-dir lib/ --ignore ./node_modules,./.babelrc,./package.json,./package-lock.json,./test --copy-files",
    "server": "node lib/bin/www",
    "clean": "rimraf ./lib && mkdir lib",
    "start": "npm run test && npm run build && npm run server",
    "server:watch": "cross-env NODE_ENV=dev nodemon --exec babel-node bin/www",
    "lint": "esw  ./test ./routes ./models/ app.js",
    "lint:watch": "npm run lint -- --watch"
  },
  "dependencies": {
    "babel-istanbul": "^0.12.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "chai-http": "^4.2.0",
    "chai-things": "^0.2.0",
    "cookie-parser": "~1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "cors": "^2.8.5",
    "cryptiles": ">=4.1.2",
    "hoek": ">=4.2.1",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.0",
    "http-errors": "~1.6.2",
    "lodash": "^4.17.13",
    "mongodb": "^3.1.8",
    "mongoose": "^5.3.8",
    "morgan": "~1.9.0",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "passport-local": "^1.0.0",
    "supertest-as-promised": "^4.0.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.2.0",
    "chalk": "^2.4.1",
    "coveralls": "^2.13.3",
    "cross-env": "^5.2.0",
    "eslint": "^5.9.0",
    "eslint-plugin-json": "^1.3.2",
    "eslint-watch": "^4.0.2",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.7",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.1.0",
    "rimraf": "^2.6.2",
    "superagent": "^4.0.0-beta.5",
    "supertest": "^3.3.0"
  }
}
